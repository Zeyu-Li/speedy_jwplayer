{
    "name": "Speedy JW Player",
    "author": "Andrew Li",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Speed up JW Video Player",
    "icons": {"128": "images/logo.png"},
    "permissions": ["activeTab"],
    "browser_action": {
        "default_icon": "images/logo.png",
        "default_popup": "popup.html",
        "default_title": "JW Video Player Controller"
    },
    "content_scripts": [ 
        {
            "matches": [
                "*://www.much.com/*"
            ],
            "js": [
                "js/content.js"
            ]
        }
    ]

}